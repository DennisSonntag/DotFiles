#!/usr/bin/env python3

import subprocess

# import math
# import json

installedVersion = subprocess.getoutput("pacman -Qet | rg linux-zen | awk '{print $2}' | sed 's/\.zen1-1$//'")
latestVersion = subprocess.getoutput("pacman -Ss linux-zen | rg \"linux-zen \" | awk '{print $2}' | sed 's/\.zen1-1$//'")

if installedVersion == latestVersion:
    print("No updates needed")
else:
    print("update form {} to {}".format(installedVersion, latestVersion))
